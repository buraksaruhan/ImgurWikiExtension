{
    "manifest_version": 2,
    "name": "Imgur and Pastebin Bypass",
    "description": "It is used primarily to bypass the blocking of Imgur and Pastebin in Turkey.",
    "version": "0.32",
    "author": "Burak Saruhan",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "activeTab",
        "http://*/*",
        "https://*/*",
        "webRequest", 
        "webRequestBlocking"
    ]
}